<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape >

            <gradient
                android:startColor="#D50000"
                android:centerColor="#DF7401"
                android:endColor="#D50000"
                android:angle="90"/>
            <padding android:left="7dp"
                android:top="7dp"
                android:right="7dp"
                android:bottom="7dp" />

            <stroke android:color="@color/colorAccent" android:dashGap="10dp" android:dashWidth="10dp"></stroke>
            <corners android:radius="20dp"></corners>
        </shape>
    </item>

    <item android:id="@id/idProgress">
        <clip>
            <shape>
                <solid android:color="@color/bgLinearInToolBar"></solid>
            </shape>
        </clip>
    </item>
</layer-list>